			<class name="in.automationtesting.practice.testCases.MyAccountLogin_TC003" />
			<class name="in.automationtesting.practice.testCases.MyAccountLogin_TC004" />
			<class name="in.automationtesting.practice.testCases.MyAccountLogin_TC005" />
			<class name="in.automationtesting.practice.testCases.MyAccountLogin_TC007" />
			<class name="in.automationtesting.practice.testCases.MyAccountLogin_TC008" />
			<class name="in.automationtesting.practice.testCases.HomePageWithThreeSliders_TC001" />
			<class name="in.automationtesting.practice.testCases.HomePageWithThreeArrivals" />
		</classes>
	</test>

